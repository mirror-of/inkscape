<spmodule>
    <name>SVGZ Input</name>
    <id>modules.input.SVGZ</id>
    <input>
        <extension>.svgz</extension>
        <mimetype>image/x-svgz</mimetype>
        <filetypename>Compressed SVG</filetypename>
        <filetypetooltip>Sodipodi native file format compressed with GZip</filetypetooltip>
    </input>
    <extension>
        <command reldir="path">gzip -cd</command>
    </extension>
</spmodule>
