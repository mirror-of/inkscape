msgid "doEffect stack test"
msgstr "test stack doEffect"

#: ../src/live_effects/effect.cpp:765
msgid "Test LPE"
msgstr "Test LPE"

#: ../src/live_effects/effect.cpp:775
msgctxt "path effect"
msgid "Dynamic stroke"
msgstr "Contorno dinamico"

#: ../src/live_effects/effect.cpp:778
